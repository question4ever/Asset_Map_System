<Window x:Class="Asset_Map_System.FloorMapWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Asset_Map_System"
        mc:Ignorable="d"
        Title="FloorMapWindow" Height="500" Width="500" Icon="spiral_icon.png"  SizeChanged="Window_SizeChanged" Loaded="Window_Loaded">
    <Canvas Name="Floor">
        <Image Name="FloorPlan" Source="{StaticResource mapDrawingImage}"></Image>
    </Canvas>
</Window>
